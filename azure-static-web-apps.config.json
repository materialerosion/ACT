{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/index.html"
    },
    "401": {
      "rewrite": "/index.html"
    },
    "403": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "cache-control": "no-store, max-age=0"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
